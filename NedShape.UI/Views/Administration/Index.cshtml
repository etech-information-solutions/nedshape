    <div id="tab-data" class="tab-data">

        <div id="roles" class="da-tab none" data-load-url="Roles"> </div>

        <div id="users" class="da-tab none" data-load-url="Users"> </div>

        <div id="auditlog" class="da-tab none" data-load-url="AuditLog"> </div>

        <div id="broadcasts" class="da-tab none" data-load-url="Broadcasts"> </div>

        <div id="banks" class="da-tab none" data-load-url="Banks"> </div>

